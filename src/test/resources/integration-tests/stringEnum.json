{
  "$schema": "http://json-schema.org/draft-06/schema#",
  "$id": "http://example.org/StringEnum",
  "properties": {
    "someProp": {
      "enum": [
          "a",
          "b"
      ]
    },
    "otherProp": {
      "properties": {
        "enumProp": {
          "enum": ["foo"]
        }
      },
      "required": ["enumProp"]
    }
  },
  "required": ["someProp", "otherProp"]
}